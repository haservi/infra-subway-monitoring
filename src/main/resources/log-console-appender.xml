<included>
    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <!-- 로그 패턴 -->
        <encoder>
            <charset>UTF-8</charset>
            <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} [%thread] %highlight(%-5level) %cyan(%logger{15}) - %msg %n</pattern>
        </encoder>
    </appender>
</included>
